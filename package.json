{
  "name": "PurchaseOrderAdvanced",
  "version": "1.0.0",
  "description": "Purchase Order",
  "repository": "PurchaseOrderAdvanced",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js-community/odata-v2-adapter": "^1.13.1",
    "@cap-js/graphql": "^0.11.0",
    "@cap-js/hana": "^1.1.1",
    "@cap-js/postgres": "^1.10.0",
    "@cap-js/sqlite": "^1.7.3",
    "@sap/cds": "^8.1.1",
    "@sap/hdbext": "^8.0.6",
    "@sap/xsenv": "^5.2.0",
    "@sap/xssec": "^4.2.2",
    "express": "^4.19.2",
    "hdb": "^0.19.9",
    "passport": "^0.7.0",
    "sap-hdbext-promisfied": "^2.202407.1",
    "cds-swagger-ui-express": "^0.9.0"
  },
  "devDependencies": {
    "@sap/hdi-deploy": "^5.2.1",
    "@sap/ux-specification": "^1.120.17",
    "rimraf": "^6.0.1"
  },
  "scripts": {
    "start": "cds-serve",
    "undeploy": "cf undeploy PurchaseOrder --delete-services --delete-service-keys --delete-service-brokers",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1"
  },
  "cds": {
    "server": {
      "index": true
    },
    "fiori": {
      "bypass_draft": true
    },
    "requires": {
      "db": "hana",
      "[hybrid]": {
        "auth": "xsuaa"
      },
      "auth": "xsuaa",
      "destinations": true,
      "connectivity": true,
      "[sqlite]": {
        "db": {
          "kind": "sqlite",
          "impl": "@cap-js/sqlite",
          "credentials": {
            "url": "db.sqlite"
          },
          "schema_evolution": "auto"
        },
        "db-ext": {
          "model": "db/sqlite"
        }
      },
      "[pg]": {
        "db": {
          "kind": "postgres",
          "dialect": "postgres",
          "impl": "@cap-js/postgres",
          "schema_evolution": "auto",
          "credentials": {
            "host": "localhost",
            "port": 5432,
            "user": "postgres",
            "password": "postgres",
            "database": "PurchaseOrderAdvanced"
          }
        },
        "db-ext": {
          "model": "db/postgres"
        }
      }
    },
    "hana": {
      "deploy-format": "hdbtable"
    }
  },
  "sapux": []
}
